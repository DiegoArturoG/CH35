package com.hackerranck.ifelse;
import java.util.Scanner;
public class ElseIfEjercicio {
	public static void main (String[] args) {

		//Scanner para leeer entrada del usuario 
		
		Scanner scan= new Scanner(System.in);
		
		//Solicitud para que el ususario ingrese un número
		System.out.println("Ingresa un numero entero del 1 al 100: ");
		
		//Declaración de varialble de tipo entero y lectura del mismo
		
		int numero = scan.nextInt();
		
		//Loop if-else
		
		if (isPar(numero)) {
			System.out.println("Not Weird");
		} else {
			System.out.println("Weird");
		}
		
		//Close scanner
		scan.close();
		
	}
		
	//Method for par or impar number 
	
	static boolean isPar(int num) {
		return num % 2 == 0;
	}
		

}
